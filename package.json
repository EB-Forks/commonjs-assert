{
  "name": "assert",
  "description": "The node.js assert module, re-packaged for web browsers.",
  "keywords": [
    "assert",
    "browser"
  ],
  "version": "1.4.1",
  "homepage": "https://github.com/defunctzombie/commonjs-assert",
  "repository": {
    "type": "git",
    "url": "git://github.com/defunctzombie/commonjs-assert.git"
  },
  "main": "./assert.js",
  "dependencies": {
    "object-assign": "^4.1.1",
    "util": "0.10.3"
  },
  "devDependencies": {
    "airtap": "^2.0.2",
    "tape": "^4.10.1"
  },
  "license": "MIT",
  "scripts": {
    "test-node": "node test.js",
    "test-browser": "airtap -- test.js",
    "test": "npm run test-node && npm run test-browser",
    "test-native": "TEST_NATIVE=true node test.js",
    "browser-local": "airtap --no-coverage --local 8000 -- test.js"
  }
}
